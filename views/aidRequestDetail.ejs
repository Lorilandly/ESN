<body>
    <div class="main-container">
        <%- include('./header.ejs') %>
        <div class="aid-request-page-header" id="aid-request-page-header">
            <button onclick="window.history.back();" >
                <i class="bi bi-arrow-left"></i> 
            </button>
            <div class="aid-request-page-header">
                Aid Request Detail
            </div>
        </div>
        <div id="aid-request-detail-page-body">
            <div class="aid-request-detail">
                <p><strong>Creator:</strong> <span id="aid-request-creator"></span></p>
                <p><strong>Title:</strong> <span id="aid-request-title"></span></p>
                <p><strong>Description:</strong> <span id="aid-request-description"></span></p>
                <p><strong>Priority:</strong> <span id="aid-request-priority"></span></p>
            </div>
        </div>
        <%- include('./footer.ejs') %>
        
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="/javascripts/aidRequestDetail.js"></script>
